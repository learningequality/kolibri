{
  "name": "kolibri",
  "version": "0.0.1",
  "description": "Development utilities for Kolibri",
  "scripts": {
    "test": "npm run test-mocha && npm run test-karma",
    "test-karma": "node ./node_modules/karma/bin/karma start ./karma_config/karma.conf.js --single-run",
    "test-mocha": "node ./node_modules/mocha/bin/mocha ./frontend_build/test/*.js",
    "coverage": "npm run test-mocha-cov && npm run test-karma-cov",
    "test-mocha-cov": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha frontend_build/test/*.js --report lcovonly -- -R spec",
    "test-karma-cov": "./node_modules/karma/bin/karma start ./karma_config/karma.conf.ci.js",
    "build": "webpack --config ./frontend_build/src/webpack.config.prod.js",
    "watch": "webpack --config ./frontend_build/src/webpack.config.dev.js --watch --progress",
    "test-karma:watch": "./node_modules/karma/bin/karma start ./karma_config/karma.conf.js",
    "bundle-stats": "webpack --config ./frontend_build/src/webpack.config.prod.js --json | node ./node_modules/webpack-bundle-size-analyzer/webpack-bundle-size-analyzer",
    "install": "node ./frontend_build/src/install_dependencies.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learningequality/kolibri.git"
  },
  "author": "Learning Equality",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/learningequality/kolibri/issues"
  },
  "private": true,
  "dependencies": {
    "babel-runtime": "6.9.2",
    "intl": "1.2.4",
    "js-cookie": "2.1.2",
    "loglevel": "1.4.1",
    "normalize.css": "4.2.0",
    "rest": "1.3.2",
    "scriptjs": "2.5.8",
    "vue": "1.0.26",
    "vue-focus": "0.1.1",
    "vue-scroll": "1.0.3",
    "vue-intl": "0.4.0",
    "vue-router": "0.7.13",
    "vuex": "0.6.3"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-plugin-transform-strict-mode": "6.11.3",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-ie": "6.6.2",
    "babel-preset-stage-3": "6.11.0",
    "colors": "1.1.2",
    "css-loader": "0.23.1",
    "eslint": "2.13.1",
    "eslint-config-airbnb": "6.2.0",
    "eslint-loader": "1.4.1",
    "eslint-plugin-html": "1.5.1",
    "eslint-plugin-react": "4.3.0",
    "esprima": "^2.7.2",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "fg-loadcss": "1.2.0",
    "file-loader": "0.8.5",
    "htmlhint": "0.9.13",
    "htmlhint-loader": "0.1.0",
    "imports-loader": "0.6.5",
    "istanbul": "0.4.4",
    "istanbul-instrumenter-loader": "0.1.3",
    "jeet": "6.1.1",
    "jshint": "2.9.2",
    "jshint-loader": "0.8.3",
    "json-loader": "0.5.4",
    "karma": "0.13.22",
    "karma-coverage": "0.5.5",
    "karma-eslint": "2.1.0",
    "karma-mocha": "0.2.2",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "lodash": "4.13.1",
    "mkdirp": "0.5.1",
    "mocha": "2.5.3",
    "phantomjs-polyfill-find": "ptim/phantomjs-polyfill-find",
    "phantomjs-prebuilt": "2.1.7",
    "postcss-loader": "0.9.1",
    "recursive-install": "1.2.0",
    "rewire": "2.5.2",
    "rewire-webpack": "1.0.1",
    "shelljs": "0.7.4",
    "sinon": "2.0.0-pre.2",
    "strip-json-comments": "2.0.1",
    "style-loader": "0.13.1",
    "stylint": "1.5.6",
    "stylint-loader": "1.0.0",
    "stylint-stylish": "1.3.2",
    "stylus": "0.54.5",
    "stylus-loader": "2.1.1",
    "svg-inline-loader": "https://github.com/learningequality/svg-inline-loader",
    "temp": "0.8.3",
    "url-loader": "0.5.7",
    "vue-hot-reload-api": "1.3.3",
    "vue-html-loader": "1.2.3",
    "vue-loader": "8.5.3",
    "vue-style-loader": "1.0.0",
    "webpack": "1.13.1",
    "webpack-bundle-size-analyzer": "2.0.2",
    "webpack-bundle-tracker": "https://github.com/learningequality/webpack-bundle-tracker",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1",
    "webpack-merge": "0.14.1"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "babel": {
    "presets": [
      "es2015-ie"
    ],
    "plugins": [
      "transform-runtime",
      "transform-strict-mode"
    ],
    "comments": false
  }
}
