{
  "name": "kolibri",
  "version": "0.0.1",
  "description": "Development utilities for Kolibri",
  "scripts": {
    "test": "yarn run test-mocha && yarn run test-karma",
    "test-karma": "node ./node_modules/karma/bin/karma start ./karma_config/karma.conf.js --single-run",
    "test-mocha": "node ./node_modules/mocha/bin/mocha ./frontend_build/test/*.js",
    "coverage": "yarn run test-mocha-cov && yarn run test-karma",
    "test-mocha-cov": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha frontend_build/test/*.js --report lcovonly -- -R spec",
    "test-karma-cov": "./node_modules/karma/bin/karma start ./karma_config/karma.conf.ci.js",
    "build": "webpack --config ./frontend_build/src/webpack.config.prod.js",
    "test-build": "yarn run lint-js && webpack --config ./frontend_build/src/webpack.config.prod.js --bail",
    "watch": "node ./frontend_build/src/webpackdevserver.js",
    "test-karma:watch": "./node_modules/karma/bin/karma start ./karma_config/karma.conf.js",
    "bundle-stats": "webpack --config ./frontend_build/src/webpack.config.prod.js --json | node ./node_modules/webpack-bundle-size-analyzer/webpack-bundle-size-analyzer",
    "install": "node ./frontend_build/src/install_dependencies.js",
    "clean": "node ./frontend_build/src/clean.js",
    "preinstall": "node ./frontend_build/src/npm_deprecation_warning.js",
    "lint-js": "node ./frontend_build/src/prettier-frontend.js --prettierPath ./.prettier.js",
    "lint-js:fix": "yarn run lint-js -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learningequality/kolibri.git"
  },
  "author": "Learning Equality",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/learningequality/kolibri/issues"
  },
  "private": true,
  "dependencies": {
    "array-includes": "^3.0.3",
    "core-js": "^2.4.1",
    "css-element-queries": "^0.4.0",
    "date-fns": "^1.28.2",
    "engine-strict": "^1.0.0",
    "filesize": "^3.3.0 ",
    "frame-throttle": "^3.0.0",
    "intl": "^1.2.4",
    "js-cookie": "^2.1.2",
    "keen-ui": "^1.0.1",
    "lodash": "^4.17.4",
    "loglevel": "^1.4.1",
    "markdown-it": "^8.3.1",
    "material-design-icons": "^3.0.1",
    "purecss": "^1.0.0",
    "rest": "^2.0.0",
    "screenfull": "^3.3.1",
    "seededshuffle": "^0.2.0",
    "vue": "^2.3.3",
    "vue-intl": "^2.0.0",
    "vue-markdown": "^2.1.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "avoriaz": "^6.0.0",
    "buble": "^0.16.0",
    "buble-loader": "^0.4.1",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.2.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "beta",
    "esprima": "^4.0.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^3.0.1",
    "fg-loadcss": "^1.2.0",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "htmlhint": "^0.9.13",
    "htmlhint-loader": "^1.3.0",
    "imports-loader": "^0.7.1",
    "intl-messageformat-parser": "^1.3.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.2",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.4.2",
    "rewire": "^2.5.2",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.4",
    "simulant": "^0.2.2",
    "sinon": "^4.0.1",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.19.0",
