{
  "name": "kolibri",
  "version": "0.0.1",
  "description": "Development utilities for Kolibri",
  "scripts": {
    "test": "yarn run test-mocha && yarn run test-karma",
    "test-karma": "node ./node_modules/karma/bin/karma start ./karma_config/karma.conf.js --single-run",
    "test-mocha": "node ./node_modules/mocha/bin/mocha ./frontend_build/test/*.js",
    "coverage": "yarn run test-mocha-cov && yarn run test-karma",
    "test-mocha-cov": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha frontend_build/test/*.js --report lcovonly -- -R spec",
    "test-karma-cov": "./node_modules/karma/bin/karma start ./karma_config/karma.conf.ci.js",
    "build": "webpack --config ./frontend_build/src/webpack.config.prod.js",
    "test-build": "yarn run lint-js && webpack --config ./frontend_build/src/webpack.config.prod.js --bail",
    "watch": "node ./frontend_build/src/webpackdevserver.js",
    "test-karma:watch": "./node_modules/karma/bin/karma start ./karma_config/karma.conf.js",
    "bundle-stats": "webpack --config ./frontend_build/src/webpack.config.prod.js --json | node ./node_modules/webpack-bundle-size-analyzer/webpack-bundle-size-analyzer",
    "install": "node ./frontend_build/src/install_dependencies.js",
    "clean": "node ./frontend_build/src/clean.js",
    "preinstall": "node ./frontend_build/src/npm_deprecation_warning.js",
    "lint-js:base": "node ./frontend_build/src/prettier-frontend.js --prettierPath ./.prettier.js",
    "lint-js": "yarn run lint-js:base -- './\\{kolibri/**/assets,frontend_build,karma_config\\}/**/*.\\{js,vue\\}'",
    "lint-js:fix": "yarn run lint-js -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learningequality/kolibri.git"
  },
  "author": "Learning Equality",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/learningequality/kolibri/issues"
  },
  "private": true,
  "dependencies": {
    "core-js": "2.4.1",
    "css-element-queries": "^0.4.0",
    "date-fns": "^1.28.2",
    "filesize": "3.3.0 ",
    "frame-throttle": "^2.0.1",
    "intl": "1.2.4",
    "js-cookie": "2.1.2",
    "keen-ui": "1.0.0",
    "lodash": "^4.17.4",
    "loglevel": "1.4.1",
    "markdown-it": "^8.3.1",
    "material-design-icons": "^3.0.1",
    "purecss": "^0.6.2",
    "rest": "1.3.2",
    "screenfull": "3.3.1",
    "seededshuffle": "^0.1.1",
    "vue": "^2.3.3",
    "vue-intl": "2.0.0",
    "vue-markdown": "^2.1.3",
    "vue-router": "2.1.1",
    "vuex": "1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "buble": "0.15.2",
    "buble-loader": "0.4.0",
    "colors": "1.1.2",
    "commander": "^2.9.0",
    "css-loader": "0.25.0",
    "esprima": "2.7.2",
    "exports-loader": "0.6.3",
    "fg-loadcss": "1.2.0",
    "file-loader": "0.9.0",
    "glob": "^7.1.2",
    "htmlhint": "0.9.13",
    "htmlhint-loader": "0.1.0",
    "imports-loader": "0.6.5",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^1.2.0",
    "json-loader": "0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "0.5.5",
    "karma-mocha": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "^0.0.30",
    "karma-webpack": "^2.0.2",
    "mkdirp": "0.5.1",
    "mocha": "2.5.3",
    "node-sass": "^4.5.0",
    "postcss-loader": "1.2.2",
    "prettier": "^1.4.2",
    "rewire": "^2.5.2",
    "sass-loader": "^5.0.1",
    "shelljs": "0.7.4",
    "simulant": "0.2.2",
    "sinon": "^2.3.5",
    "strip-json-comments": "2.0.1",
    "style-loader": "0.13.1",
    "stylint": "1.5.6",
    "stylint-loader": "1.0.0",
    "stylint-stylish": "1.3.2",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "svg-icon-inline-loader": "3.0.0",
    "temp": "0.8.3",
    "url-loader": "0.5.7",
    "vue-hot-reload-api": "2.0.6",
    "vue-html-loader": "1.2.3",
    "vue-loader": "https://github.com/vuejs/vue-loader/",
    "vue-style-loader": "1.0.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "2.2.1",
    "webpack-bundle-size-analyzer": "2.0.2",
    "webpack-bundle-tracker": "https://github.com/learningequality/webpack-bundle-tracker",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "2.3.0",
    "webpack-hot-middleware": "2.12.1",
    "webpack-merge": "^2.5.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
