    "esprima": "^4.0.0",
    "exports-loader": "^0.6.3",
    "fg-loadcss": "^1.2.0",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "htmlhint": "^0.9.13",
    "htmlhint-loader": "^1.3.0",
    "imports-loader": "^0.7.1",
    "intl-messageformat-parser": "^1.3.0",
    "jest": "^23.6.0",
