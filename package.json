{
  "name": "kolibri",
  "version": "0.0.1",
  "description": "Development utilities for Kolibri",
  "scripts": {
    "test": "npm run test-mocha && npm run test-karma",
    "test-karma": "node ./node_modules/karma/bin/karma start ./karma_config/karma.conf.js --single-run",
    "test-mocha": "node ./node_modules/mocha/bin/mocha ./frontend_build/test/*.js",
    "coverage": "npm run test-mocha-cov && npm run test-karma-cov",
    "test-mocha-cov": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha frontend_build/test/*.js --report lcovonly -- -R spec",
    "test-karma-cov": "./node_modules/karma/bin/karma start ./karma_config/karma.conf.ci.js",
    "build": "webpack --config ./frontend_build/src/webpack.config.prod.js",
    "watch": "webpack --config ./frontend_build/src/webpack.config.dev.js --watch --progress",
    "test-karma:watch": "./node_modules/karma/bin/karma start ./karma_config/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learningequality/kolibri.git"
  },
  "author": "Learning Equality",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/learningequality/kolibri/issues"
  },
  "private": true,
  "dependencies": {
    "babel-runtime": "^6.0.0",
    "intl": "^1.2.3",
    "loglevel": "^1.4.0",
    "normalize.css": "^4.1.1",
    "pdfobject": "^2.0.201604172",
    "vue": "^1.0.21",
    "vue-intl": "^0.3.0",
    "vue-router": "^0.7.13",
    "vuex": "^0.6.3",
    "vuex-router-sync": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-3": "^6.0.0",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.0.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.3.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fg-loadcss": "^1.2.0",
    "file-loader": "^0.8.5",
    "htmlhint": "^0.9.13",
    "htmlhint-loader": "^0.1.0",
    "imports-loader": "^0.6.5",
    "istanbul": "^0.4.2",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jshint": "^2.9.1",
    "jshint-loader": "^0.8.3",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-eslint": "^2.1.0",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.6.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "phantomjs-polyfill-find": "ptim/phantomjs-polyfill-find",
    "phantomjs-prebuilt": "^2.1.4",
    "rewire": "^2.5.1",
    "rewire-webpack": "^1.0.1",
    "scriptjs": "^2.5.8",
    "sinon": "^2.0.0-pre",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.13.1",
    "stylint": "^1.3.10",
    "stylint-loader": "^1.0.0",
    "stylint-stylish": "^1.2.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.0",
    "temp": "^0.8.3",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^8.5.2",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.12",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-merge": "^0.8.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-strict-mode"
    ],
    "comments": false
  }
}
